SELECT  min(DATETIME)
FROM ANIMAL_INS 

SELECT count(ANIMAL_ID)
FROM ANIMAL_INS 

SELECT count(distinct(name))
FROM ANIMAL_INS 
============================================================================================================
=================================================================================
SELECT  ANIMAL_TYPE, count(*)
FROM  ANIMAL_INS 
GROUP BY ANIMAL_TYPE
ORDER BY ANIMAL_TYPE --> CAT 먼저 이후 DOG

SELECT NAME,COUNT(*)
FROM ANIMAL_INS 
GROUP BY NAME
HAVING COUNT(NAME)>=2
ORDER BY NAME;


SELECT hour,count(*)
FROM (
        SELECT to_char(DATETIME,'hh24') hour    -->'HH24' OK
        FROM ANIMAL_OUTS 
    )
WHERE hour>=9 and hour<20
group by hour
order by hour;

