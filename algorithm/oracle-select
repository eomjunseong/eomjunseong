SELECT ANIMAL_ID ,NAME
FROM ANIMAL_INS 
WHERE INTAKE_CONDITION != 'Aged';--> " " XXXX ///' ' OK --> Aged 였음 , 정확히 치기 aged X


SELECT ANIMAL_ID,NAME,DATETIME
FROM ANIMAL_INS
ORDER BY NAME ASC, DATETIME DESC; 
//이름 오름 차순 Raven->Sugar  ASC,  날짜 내림 차순 2016->2015 DESC 

제일 DATETIME 이 느린거 뽑기
SELECT NAME
FROM (SELECT * FROM ANIMAL_INS ORDER BY DATETIME)
WHERE rownum = 1;

======================================================================
SELECT DATETIME
FROM (SELECT * 
      FROM   ANIMAL_INS  
      ORDER BY DATETIME DESC) --> DESC;)XXXX 
WHERE ROWNUM = 1;
//SAME RESULT
SELECT MAX(DATETIME)
FROM ANIMAL_INS 
======================================================================



SELECT ANIMAL_ID ,NAME
FROM  ANIMAL_INS 
WHERE INTAKE_CONDITION = 'Sick' --> " " 했다가틀렸고// ==가 아니라 = 임 !!!!!
ORDER BY ANIMAL_ID ;






