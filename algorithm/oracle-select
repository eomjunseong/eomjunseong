SELECT ANIMAL_ID
FROM  ANIMAL_INS 
WHERE  NAME is NULL  ///WHERE NAME is not NULL OK
ORDER BY ANIMAL_ID ASC;

SELECT ANIMAL_TYPE, nvl(NAME,'No name'),SEX_UPON_INTAKE
FROM ANIMAL_INS
ORDER BY ANIMAL_ID
--> 이름 null 이면 (NULLABLE=TRUE) nvl(,' ')
